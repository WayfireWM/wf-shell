<?xml version="1.0"?>
<wf-shell>
	<plugin name="panel">
	<_short>Panel</_short>
	<category>Shell</category>
	<group>
	<_short>General</_short>
	<option name="css_path" type="string">
		<_short>CSS Path</_short>
		<default></default>
	</option>
	<option name="widgets_left" type="string">
		<_short>Widgets Left</_short>
		<default>menu spacing4 launchers window-list</default>
	</option>
	<option name="widgets_center" type="string">
		<_short>Widgets Center</_short>
		<default>none</default>
	</option>
	<option name="widgets_right" type="string">
		<_short>Widgets Right</_short>
		<default>volume network battery clock</default>
	</option>
	<option name="minimal_height" type="int">
		<_short>Minimal Height</_short>
		<default>24</default>
	</option>
	<option name="autohide" type="bool">
		<_short>Autohide</_short>
		<default>false</default>
	</option>
	<option name="autohide_duration" type="int">
		<_short>Autohide Duration</_short>
		<default>300</default>
	</option>
	<option name="position" type="string">
		<_short>Panel Position</_short>
		<default>top</default>
		<desc>
			<value>top</value>
			<_name>Top</_name>
		</desc>
		<desc>
			<value>bottom</value>
			<_name>Bottom</_name>
		</desc>
	</option>
	<option name="edge_offset" type="int">
		<_short>Edge offset</_short>
		<_long>The distance from the cursor to the edge of screen to show the panel when it's hidden.</_long>
		<default>20</default>
	</option>
	<option name="layer" type="string">
		<_short>Layer</_short>
		<default>top</default>
		<desc>
			<value>top</value>
			<_name>Top</_name>
		</desc>
		<desc>
			<value>bottom</value>
			<_name>Bottom</_name>
		</desc>
		<desc>
			<value>overlay</value>
			<_name>Overlay</_name>
		</desc>
		<desc>
			<value>background</value>
			<_name>Background</_name>
		</desc>
	</option>
	<option name="background_color" type="string">
		<_short>Background Color</_short>
		<default>gtk_headerbar</default>
	</option>
	</group>
	<group>
	<_short>Launchers</_short>
	<option name="launchers_spacing" type="int">
		<_short>Launchers Spacing</_short>
		<default>4</default>
	</option>
	<option name="launchers_size" type="int">
		<_short>Launchers Size</_short>
		<default>42</default>
	</option>
	<option name="launchers_animation_duration" type="int">
		<_short>Animation Duration</_short>
		<_long>High values may cause jittery animations</_long>
		<default>200</default>
	</option>
	</group>
	<group>
	<_short>Clock</_short>
	<option name="clock_format" type="string">
		<_short>Clock Format</_short>
		<default>%e %A %H:%M</default>
	</option>
	<option name="clock_font" type="string">
		<_short>Clock Font</_short>
		<default>default</default>
	</option>
	</group>
	<group>
	<_short>Battery</_short>
	<option name="battery_status" type="string">
		<_short>Battery Status</_short>
		<default>percentage</default>
		<desc>
			<value>icon</value>
			<_name>Icon</_name>
		</desc>
		<desc>
			<value>percentage</value>
			<_name>Icon with Percentage</_name>
		</desc>
		<desc>
			<value>full</value>
			<_name>Full Information</_name>
		</desc>
	</option>
	<option name="battery_icon_size" type="int">
		<_short>Battery Icon Size</_short>
		<default>32</default>
		<min>1</min>
	</option>
	<option name="battery_icon_invert" type="bool">
		<_short>Battery Icon Invert Color</_short>
		<default>true</default>
	</option>
	<option name="battery_font" type="string">
		<_short>Battery Font</_short>
		<default>default</default>
	</option>
	</group>
	<group>
	<_short>Network</_short>
	<option name="network_status" type="string">
		<_short>Network Status</_short>
		<default>connection</default>
		<desc>
			<value>none</value>
			<_name>None</_name>
		</desc>
		<desc>
			<value>connection</value>
			<_name>Connection</_name>
		</desc>
		<desc>
			<value>full</value>
			<_name>Connection with IP</_name>
		</desc>
	</option>
	<option name="network_status_font" type="string">
		<_short>Clock Font</_short>
		<default>default</default>
	</option>
	<option name="network_icon_size" type="int">
		<_short>Network Icon Size</_short>
		<default>32</default>
		<min>1</min>
	</option>
	<option name="network_icon_invert_color" type="bool">
		<_short>Network Icon Invert Color</_short>
		<default>false</default>
	</option>
	<option name="network_status_use_color" type="bool">
		<_short>Network Status Use Color</_short>
		<default>false</default>
	</option>
	<option name="network_onclick_command" type="string">
		<_short>On Click Command</_short>
		<default>default</default>
	</option>
	</group>
	<group>
	<_short>Menu</_short>
	<option name="menu_fuzzy_search" type="bool">
		<_short>Use Fuzzy Search in Menu</_short>
		<default>true</default>
	</option>
	<option name="menu_icon" type="string">
		<_short>Menu Icon</_short>
		<hint>file</hint>
		<default></default>
	</option>
	<option name="menu_min_content_width" type="int">
		<_short>Menu Minimum Content Width</_short>
		<default>500</default>
		<min>100</min>
		<max>5000</max>
	</option>
	<option name="menu_min_content_height" type="int">
		<_short>Menu Minimum Content Height</_short>
		<default>500</default>
		<min>100</min>
		<max>5000</max>
	</option>
	<option name="menu_logout_command" type="string">
		<_short>Menu Logout Command</_short>
		<_long>If not specified, a default logout interface will be used.</_long>
		<default></default>
	</option>
	<option name="logout_command" type="string">
		<_short>Logout Button Command</_short>
		<default>wayland-logout</default>
	</option>
	<option name="reboot_command" type="string">
		<_short>Reboot Button Command</_short>
		<default>systemctl reboot</default>
	</option>
	<option name="shutdown_command" type="string">
		<_short>Shutdown Button Command</_short>
		<default>systemctl poweroff</default>
	</option>
	<option name="suspend_command" type="string">
		<_short>Suspend Button Command</_short>
		<default>systemctl suspend</default>
	</option>
	<option name="hibernate_command" type="string">
		<_short>Hibernate Button Command</_short>
		<default>systemctl hibernate</default>
	</option>
	<option name="switchuser_command" type="string">
		<_short>Switch User Button Command</_short>
		<default>dm-tool switch-to-greeter</default>
	</option>
	</group>
	<group>
	<_short>Volume</_short>
	<option name="volume_display_timeout" type="double">
		<_short>Volume Display Timeout</_short>
		<default>2.5</default>
	</option>
	<option name="volume_icon_size" type="int">
		<_short>Volume Icon Size</_short>
		<default>32</default>
		<min>1</min>
	</option>
	</group>
	<group>
	<_short>Notifications</_short>
	<option name="notifications_autohide_timeout" type="double">
		<_short>Notifications Display Timeout</_short>
		<_long>Set to negative value to disable.</_long>
		<default>2.5</default>
	</option>
	<option name="notifications_critical_in_dnd" type="bool">
		<_short>Always Show Critical notifications</_short>
		<_long>Makes critical notifications to be shown even in DND mode.</_long>
		<default>true</default>
	</option>
	<option name="notifications_icon_size" type="int">
		<_short>Notifications Icon Size</_short>
		<default>32</default>
		<min>1</min>
	</option>
	</group>
	<group>
	<_short>Tray</_short>
	<option name="tray_smooth_scrolling_threshold" type="int">
		<_short>Smooth Scrolling Threshold</_short>
		<default>5</default>
		<min>1</min>
	</option>
	<option name="tray_icon_size" type="int">
		<_short>Default Icon Size</_short>
		<default>32</default>
		<min>1</min>
	</option>
	<option name="tray_menu_on_middle_click" type="bool">
		<_short>Middle Button Activates Menu</_short>
		<_long>Mouse right button activates a tray item's secondary action instead of the item's menu.</_long>
		<default>false</default>
	</option>
	</group>
	<group>
	<_short>Command Output</_short>
	<option name="commands_output_max_chars" type="int">
		<_short>Max Characters To Display</_short>
		<_long>Max characters of command output to display.</_long>
		<min>0</min>
		<default>10</default>
	</option>
	<option name="commands" type="dynamic-list" type-hint="dict">
		<short>Commands to run</short>
		<entry prefix="command_output_" type="string" name="command">
			<_short>Command</_short>
			<_long>Command to run</_long>
		</entry>
		<entry prefix="command_output_tooltip_" type="string" name="tooltip_command">
			<_short>Tooltip Command</_short>
			<_long>The tooltip command is executed every time the tooltip is shown.</_long>
			<default/>
		</entry>
		<entry prefix="command_output_period_" type="int" name="period">
			<_short>Period</_short>
			<_long>
The command is launched cyclically with given period in seconds, updating the text displayed on the panel.
Set to 0 to only run the command once on startup.
Set to -1 to only run it by clicking the button.
			</_long>
			<default>0</default>
			<min>-1</min>
		</entry>
		<entry prefix="command_output_icon_" type="string" name="icon">
			<_short>Icon</_short>
			<_long>Icon name to show. Leave empty to hide icon.</_long>
			<default>text-x-script</default>
		</entry>
		<entry prefix="command_output_icon_size_" type="int" name="icon_size">
			<_short>Icon Size</_short>
			<default>32</default>
			<min>1</min>
		</entry>
		<entry prefix="command_output_icon_position_" type="string" name="icon_position">
			<_short>Icon Position</_short>
			<desc>
				<value>top</value>
				<_name>Above Text</_name>
			</desc>
			<desc>
				<value>bottom</value>
				<_name>Below Text</_name>
			</desc>
			<desc>
				<value>left</value>
				<_name>Left</_name>
			</desc>
			<desc>
				<value>right</value>
				<_name>Right</_name>
			</desc>
			<default>left</default>
		</entry>
	</option>
	</group>
	</plugin>
</wf-shell>
